dialog v-model=\"dialog\" persistent max-width=\"600px\">\n        <template v-slot:activator=\"{ on }\">\n          <v-btn color=\"primary\" dark v-on=\"on\">Novo Usu치rio</v-btn>\n        </template>\n        <v-card>\n          <v-card-title>\n            <span class=\"headline\">Cadastro de Usu치rio</span>\n          </v-card-title>\n          <v-card-text>\n            <v-container>\n              <v-row>\n                <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                  <v-text-field label=\"Nome do Usu치rio*\" required></v-text-field>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-text-field label=\"Email*\" required></v-text-field>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-text-field label=\"Password*\" type=\"password\" required></v-text-field>\n                </v-col>\n              </v-row>\n              <input type=\"checkbox\" id=\"checkbox\" v-model=\"checked\">\n              <label for=\"checkbox\">Ativo</label>\n            </v-container>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Salvar</v-btn>\n            <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Cancelar</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </v-row>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport VCardWidget from \"@/components/VWidget\";\nimport { RepositoryFactory } from \"@/repositories/RepositoryFactory\";\nconst usuarioRepo = RepositoryFactory.get(\"usuario\");\n\nexport default {\n  name: \"home\",\n\n  data: () => ({\n    headers: [\n      {\n        text: \"Nome\",\n        align: \"left\",\n        value: \"nome\"\n      },\n      { text: \"Email\", value: \"email\" },\n      { text: \"Status\", value: \"status\" },\n      { text: \"\", value: \"action\" }\n    ],\n    usuarios: []\n  }),\n\n  created() {\n    usuarioRepo\n      .getAll()\n      .then(res => {\n        this.usuarios = res.data;\n      })\n      .catch(console.error);\n  },\n\n  computed: {},\n\n  methods: {}\n};\n</script>\n\n<style>\n</style>\n"]}]}